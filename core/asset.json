{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "image",
        "video",
        "audio"
      ]
    },
    "src": {
      "$ref": "./url.json"
    },
    "alt": {
      "type": "string"
    }
  },
  "required": [
    "type",
    "src"
  ]
}