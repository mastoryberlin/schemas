{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "$schema": {
      "type": "string"
    },
    "gpt-3.5-turbo-1106": {
      "$ref": "#/$defs/modelInfo"
    },
    "gpt-3.5-turbo-instruct": {
      "$ref": "#/$defs/modelInfo"
    },
    "gpt-4-0613": {
      "$ref": "#/$defs/modelInfo"
    },
    "gpt-4-32k-0613": {
      "$ref": "#/$defs/modelInfo"
    },
    "gpt-4-1106-preview": {
      "$ref": "#/$defs/modelInfo"
    },
    "gpt-4-1106-vision-preview": {
      "$ref": "#/$defs/modelInfo"
    },
    "gpt-4o": {
      "$ref": "#/$defs/modelInfo"
    },
    "gpt-4o-mini": {
      "$ref": "#/$defs/modelInfo"
    }
  },
  "$defs": {
    "modelName": {
      "enum": [
        "gpt-3.5-turbo-1106",
        "gpt-3.5-turbo-instruct",
        "gpt-4-0613",
        "gpt-4-32k-0613",
        "gpt-4-1106-preview",
        "gpt-4-1106-vision-preview",
        "gpt-4o",
        "gpt-4o-mini"
      ]
    },
    "modelInfo": {
      "type": "object",
      "properties": {
        "price": {
          "type": "object",
          "description": "Price in US-$ per 1k tokens",
          "properties": {
            "input": {
              "type": "number",
              "exclusiveMinimum": 0
            },
            "output": {
              "type": "number",
              "exclusiveMinimum": 0
            }
          },
          "required": [
            "input",
            "output"
          ]
        },
        "tokenLimit": {
          "type": "integer",
          "description": "Max tokens allowed for this model in 1k",
          "exclusiveMinimum": 0
        }
      },
      "required": [
        "price",
        "tokenLimit"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "gpt-3.5-turbo-1106",
    "gpt-3.5-turbo-instruct",
    "gpt-4-0613",
    "gpt-4-32k-0613",
    "gpt-4-1106-preview",
    "gpt-4-1106-vision-preview",
    "gpt-4o",
    "gpt-4o-mini"
  ]
}