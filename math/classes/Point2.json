{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": [
    {
      "const": "Point2"
    },
    {
      "type": "object",
      "properties": {
        "id": {
          "$ref": "../../core/identifier.json"
        },
        "groups": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "x": {
          "type": "number"
        },
        "y": {
          "type": "number"
        },
        "color": {
          "$ref": "../../core/color.json"
        }
      },
      "additionalProperties": false,
      "required": [
        "id"
      ]
    }
  ],
  "minItems": 2,
  "maxItems": 2
}