{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "allOf": [
    {
      "$ref": "../partial/shared.json"
    },
    {
      "properties": {
        "class": {
          "const": "RenderableMath"
        },
        "substituteNumbers": {
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z]$": {
              "type": "number"
            }
          },
          "additionalProperties": false
        }
      }
    }
  ],
  "required": [
    "substituteNumbers"
  ],
  "unevaluatedProperties": false
}