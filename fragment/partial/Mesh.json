{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "allOf": [
    {
      "$ref": "./Transformable3.json"
    },
    {
      "properties": {
        "name": {
          "type": "string"
        },
        "materialId": {
          "type": "string"
        },
        "hasVertexAlpha": {
          "type": "boolean",
          "default": false
        },
        "visibility": {
          "type": "number",
          "default": 1
        },
        "isVisible": {
          "type": "boolean",
          "default": false
        },
        "isEnabled": {
          "type": "boolean",
          "default": false
        },
        "checkCollisions": {
          "type": "boolean",
          "default": false
        },
        "billboardMode": {
          "type": "number",
          "default": 0
        },
        "receiveShadows": {
          "type": "boolean",
          "default": false
        }
      }
    }
  ]
}