{
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1,
      "pattern": "^[a-zA-Z_][a-zA-Z_0-9]*$"
    },
    "blockedBy": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "support": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "type": "object",
          "patternProperties": {
            "^S_": {
              "type": "object",
              "properties": {
                "order": {
                  "type": "number"
                },
                "flow": {
                  "type": "string"
                },
                "text": {
                  "type": "string"
                },
                "inheritFrom": {
                  "type": "string"
                },
                "noNeedIndicatesProficiencyIn": {
                  "type": "array",
                  "items": {
                    "type": "object"
                  }
                }
              }
            }
          }
        }
      ]
    },
    "initiallyAbsent": {
      "type": "boolean",
      "default": true
    },
    "initiallyFrozen": {
      "type": "boolean",
      "default": true
    },
    "is": {
      "$ref": "./is-statement.json"
    },
    "asSupportIntent": {
      "oneOf": [
        {
          "$ref": "./as-intent.json"
        },
        {
          "type": "array",
          "items": {
            "$ref": "./as-intent.json"
          }
        }
      ]
    }
  },
  "required": []
}